        {% assign scripttags = scripttags[1] %}
        {% assign scripttags = scripttags | split: '"];' %}
        {% if scripttags.size > 1 %}
          {% assign scripttags = scripttags[0] %}
          {% assign scripttags = scripttags | split: '","' %}
